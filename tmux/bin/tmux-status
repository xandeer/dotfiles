FULL=▓
EMPTY=░
EOL=▒
SIZE=7
C0="#000000"
C1="#222222"
C2="#1C596E"
C3="#B3291C"
C4="#3A3A3A"
C5="#efefef"
C6="#878787"
C7="#666666"

clock() {
    mmon=$(date +'%b')
    mweek=$(date +'%a')
	mday=$(date +'%d')
	echo "#[fg=$C7]#[bg=$C4] $mmon#[fg=$C6] $mweek $mday#[fg=$C5] $mtime #[fg=$C6]$EOL"
}

front() {
	echo "#[bg=$C7]#[fg=0]▓░"
}

echo `front` `clock`
