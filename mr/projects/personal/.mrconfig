[dotfiles]
checkout = git clone 'git@github.com:xandeer/dotfiles.git' 'dotfiles'
commit = [ -n "$(git status --porcelain)" ] && git add --all && git commit -m "$(date +'manual: [%F %a %T]')" ; git push

